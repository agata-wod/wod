use uo;

program textcmd_upgradeaccounts(who)
  var count := 0;
  var acct := 0;
  var res := 0;
  foreach acctname in ListAccounts()
    acct := FindAccount(acctname);
    if(res := acct.set_uo_expansion("HSA"))
		count := count + 1;
	else
		SendSysMessage(who, res.errortext);
	endif
  endforeach
  SendSysMessage(who, count + " account(s) upgraded.");
endprogram